package dto;

public class Shop 
{
	private int shopId,  roomCount;
	private double dongkyung, bukwi;
	private String shopName, tel, hour, holiday, yesno1, yesno2;
	public int getShopId() {
		return shopId;
	}
	public void setShopId(int shopId) {
		this.shopId = shopId;
	}
	public double getDongkyung() {
		return dongkyung;
	}
	public void setDongkyung(double dongkyung) {
		this.dongkyung = dongkyung;
	}
	public double getBukwi() {
		return bukwi;
	}
	public void setBukwi(double bukwi) {
		this.bukwi = bukwi;
	}
	public int getRoomCount() {
		return roomCount;
	}
	public void setRoomCount(int roomCount) {
		this.roomCount = roomCount;
	}
	public String getShopName() {
		return shopName;
	}
	public void setShopName(String shopName) {
		this.shopName = shopName;
	}
	public String getTel()
	{
		return tel;
	}
	public void setTel(String tel)
	{
		this.tel = tel;
	}
	public String getHour()
	{
		return hour;
	}
	public void setHour(String hour)
	{
		this.hour = hour;
	}
	public String getHoliday()
	{
		return holiday;
	}
	public void setHoliday(String holiday)
	{
		this.holiday = holiday;
	}
	public String getYesno1()
	{
		return yesno1;
	}
	public void setYesno1(String yesno1)
	{
		this.yesno1 = yesno1;
	}
	public String getYesno2()
	{
		return yesno2;
	}
	public void setYesno2(String yesno2)
	{
		this.yesno2 = yesno2;
	}
	
	
	
	
	
	
}
